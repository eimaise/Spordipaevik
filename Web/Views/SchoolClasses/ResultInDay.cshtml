@model IEnumerable<Core.Data.Entities.Result>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<table  class="table table-striped table-bordered nowrap" style="width: 100%">
    <thead>
    <tr>
        <th>Nimi</th>
        <th>Tulemus</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var result in Model.OrderByDescending(x=>x.ResultValue.Value))
    {
        <tr>
            
            <td>
                <p>@result.Student.Name</p>

            </td>
            <td>
                <p>@result.ResultValue.Value</p>

            </td>
        </tr>
    }
    </tbody>
</table>

